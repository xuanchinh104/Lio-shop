{
    "name": "asc",
    "version": "0.0.0",
    "license": "MIT",
    "scripts": {
        "ng": "node --max_old_space_size=4096 ./node_modules/.bin/ng",
        "nx": "node --max_old_space_size=8092 ./node_modules/@nrwl/cli/bin/nx",
        "postinstall": "node ./decorate-angular-cli.js && ngcc --properties es2015 browser module main",
        "start": "nx serve",
        "build": "nx build flower-shop --stats-json",
        "start:admin": "nx serve flower-shop --port 8080",
        "start:mobile": "nx serve config-mobile --port 8081",
        "start:web": "nx serve flower-shop-web --port 9000",
        "build:admin": "NG_PERSISTENT_BUILD_CACHE=1 node --max_old_space_size=8192 ./node_modules/.bin/ng build flower-shop --prod",
        "build:mobile": "NG_PERSISTENT_BUILD_CACHE=1 ng build config-mobile --prod",
        "build:web": "NG_PERSISTENT_BUILD_CACHE=1 node --max_old_space_size=8192 ./node_modules/.bin/ng build flower-shop-web --prod",
        "affected:build": "./node_modules/.bin/nx affected:build flower-shop --prod --base=origin/master --head=HEAD --aot --parallel",
        "test": "ng test --coverage --log-heap-usage -w=2",
        "lint": "nx eslint",
        "prettier:format": "nx format:write --all",
        "prettier:check": "nx format:check --all",
        "analyze:admin": "webpack-bundle-analyzer dist/apps/flower-shop/stats.json",
        "analyze:web": "webpack-bundle-analyzer dist/apps/flower-shop-web/stats.json"
    },
    "private": true,
